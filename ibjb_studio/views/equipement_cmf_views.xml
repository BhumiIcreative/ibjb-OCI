<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <!--custom form view equipement cmf-->
    <record id="equipement_cmf_form_view" model="ir.ui.view">
        <field name="name">equipement.cmf.form.view</field>
        <field name="model">equipement.cmf</field>
        <field name="arch" type="xml">
            <form>
                <sheet string="Equipement CMF">
                    <div class="oe_title">
                        <h1>
                            <field name="name" required="1"/>
                        </h1>
                    </div>
                    <group name="group_e23182">
                        <group name="group_e23182_left"/>
                        <group name="group_e23182_right"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <!--Action for the equipement cmf model-->
    <record id="action_equipement_cmf" model="ir.actions.act_window">
        <field name="name">Equipement CMF</field>
        <field name="res_model">equipement.cmf</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                This is your new action ; by default, it contains a list view and a form view.
            </p>
            <p>
                You can start customizing these screens by clicking on the Studio icon on the
                top right corner (you can also customize this help message there).
            </p>
        </field>
    </record>
    <menuitem action="action_equipement_cmf" id="ibjb_equipement_cmf" parent="contacts.res_partner_menu_config"/>
</odoo>