<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="report_inventory_inherit" inherit_id="stock.report_inventory">
            <xpath expr="//table[1]/thead/tr/th[2]" position="before">
                <th>
                    <strong>Product reference</strong>
                </th>
            </xpath>
            <xpath expr="//table[1]/tbody/t[1]/tr[2]/td[2]" position="before">
                <td><span t-field="line.ref_interne_1"></span></td>
            </xpath>
            <xpath expr="//table[1]/thead/tr/th[4]" position="after">
                <th>
                    <strong>Expiration date</strong>
                </th>
            </xpath>
            <xpath expr="//table[1]/tbody/t[1]/tr[2]/td[4]" position="after">
                <td><span t-field="line.field_WfwqV"></span></td>
            </xpath>
            <xpath expr="//table[1]/thead/tr" position="inside">
                <th>
                    <strong>Possible comment</strong>
                </th>
            </xpath>
            <xpath expr="//table[1]/tbody/t[1]/tr[2]" position="inside">
                <td><span t-field="line.oci_inventaire_note"></span></td>
            </xpath>
        </template>
    </data>
</odoo>